  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "bootstrap": "^4.4.1",
    "daemonite-material": "^4.1.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "parallelshell": "^3.0.1"
  }
}
